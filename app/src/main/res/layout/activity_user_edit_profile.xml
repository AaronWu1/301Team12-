<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/button_save_EditUserProfileActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dada.View.UserEditProfileActivity">

    <EditText
        android:id="@+id/editText_email_EditUserProfileActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Email"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="132dp" />

    <EditText
        android:id="@+id/editText_mobile_EditUserProfileActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        android:text="Phone"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="215dp" />

    <EditText
        android:id="@+id/editText_userName_EditUserProfileActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="49dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Save"
        tools:layout_editor_absoluteX="129dp"
        tools:layout_editor_absoluteY="339dp" />


</android.support.constraint.ConstraintLayout>
